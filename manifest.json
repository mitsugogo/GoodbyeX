{
    "name": "GoodbyeX - customized by mitsugogo (base v1.0.2)",
    "version": "1.0.2",
    "description": "A simple extension that removes the new \"X\" branding from Twitter.",
    "manifest_version": 3,
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*",
                "https://x.com/*"
            ],
            "js": [
                "lib.js",
                "script.js"
            ],
            "css": [],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://tweetdeck.twitter.com/*"
            ],
            "js": [
                "lib.js",
                "script_tweetdeck.js"
            ],
            "css": [],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://google.com/*",
                "https://www.google.com/*"
            ],
            "js": [
                "lib.js",
                "script_google.js"
            ],
            "css": [],
            "run_at": "document_start"
        }
    ],
    "homepage_url": "https://twitter.com/mitsugogo"
}